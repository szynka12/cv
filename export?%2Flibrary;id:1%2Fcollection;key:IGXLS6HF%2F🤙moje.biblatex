@article{schulze,
  title = {Effects of {{Streamwise Oriented Riblets}} on an {{LPT Blade}}: {{A Numerical Study}}},
  author = {Schulze, Sebastian and Presti, Federico Lo and Sadowski, Wojciech and family=Mare, given=Francesca, prefix=di, useprefix=true},
  abstract = {The flexible use of hydrogen in energy storage and conversion applications necessitates machines with many axial stages. The cost challenges of blade production for multi-stage machines can be alleviated by the use of the recent advancements in manufacturing technologies, such as additive manufacturing and milling. This study analyses the effects of the surface features introduced by the new manufacturing technologies, which are often neglected or simplified in numerical simulations. The investigation focuses on the aft-loaded blade profile, T106RUB, which has been modified to match the characteristics of the original T106 profile under low-speed conditions. Additionally, the blade surface has been modified to feature streamwise oriented riblets, thereby modelling the influence of the manufacturing process. The T106RUBprofile, previously studied both in experimental and simulated capacity, provides a strong reference for analysing the riblets’ effects. The investigation is carried out using highly-resolved Large Eddy Simulations (LES) using SharC in-house solver. Results are compared with a smooth T106RUBprofile, focusing on time-averaged quantities like wall shear stress and the overlaying 3D flow structures using the Q-Criterion. The results reveal the formation of counter-rotating elongated vortices influenced by rib structures and free stream turbulence. The ribs have a counteracting effect on the wall shear stress for the pressure and suction sides, which is more pronounced on the pressure side while the suction side remains largely unaffected.},
  langid = {english},
  file = {/home/szynka/Zotero/storage/FGGQ2YI4/Schulze et al. - Effects of Streamwise Oriented Riblets on an LPT Blade A Numerical Study.pdf}
}

@article{suga2010,
  title = {Effects of Wall Permeability on Turbulence},
  author = {Suga, K. and Matsumura, Y. and Ashitaka, Y. and Tominaga, S. and Kaneda, M.},
  date = {2010-12},
  journaltitle = {International Journal of Heat and Fluid Flow},
  shortjournal = {International Journal of Heat and Fluid Flow},
  volume = {31},
  number = {6},
  pages = {974--984},
  issn = {0142727X},
  doi = {10.1016/j.ijheatfluidflow.2010.02.023},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0142727X10000561},
  urldate = {2022-01-02},
  abstract = {In order to understand the effects of the wall permeability on turbulence near a porous wall, flow field measurements are carried out for turbulent flows in a channel with a porous bottom wall by a two-component particle image velocimetry (PIV) system. The porous media used are three kinds of foamed ceramics which have almost the same porosity (\$0.8) but different permeability. It is confirmed that the flow becomes more turbulent over the porous wall and tends to be turbulent even at the bulk Reynolds number of Reb ¼ 1300 in the most permeable wall case tested. Corresponding to laminar to turbulent transition, the magnitude of the slip velocity on the porous wall is found to increase drastically in a narrow range of the Reynolds number. To discuss the effects of the wall roughness and the wall permeability, detailed discussions are made of zero-plane displacement and equivalent wall roughness for porous media. The results clearly indicate that the turbulence is induced by not only the wall roughness but the wall permeability. The measurements have also revealed that as Reb or the wall permeability increases, the wall normal fluctuating velocity near the porous wall is enhanced due to the effects of the wall permeability. This leads to the increase of the turbulent shear stress resulting in higher friction factors of turbulence over porous walls.},
  langid = {english},
  file = {/home/szynka/Zotero/storage/MUZ5RXLR/Suga et al. - 2010 - Effects of wall permeability on turbulence.pdf}
}

