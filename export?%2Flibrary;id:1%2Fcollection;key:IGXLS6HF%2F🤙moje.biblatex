  shorttitle = {Modeling {{Gas Flows}} in {{Packed Beds}} with the {{Lattice Boltzmann Method}}},
  author = {Neeraj, Tanya and Velten, Christin and Zähringer, Katharina and Namdar, Reza and Varnik, Fathollah and Thévenin, Dominique and Hosseini, Seyed Ali},
